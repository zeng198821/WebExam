<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zeng.dao.StudentinfoEntity" table="studentinfo" schema="exam">
        <id name="studentId">
            <column name="studentId" sql-type="int(11)"/>
        </id>
        <property name="studentName">
            <column name="studentName" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="capId">
            <column name="capId" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="classId">
            <column name="classId" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="phoneNo">
            <column name="phoneNo" sql-type="varchar(12)" length="12" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>