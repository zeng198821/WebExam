<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zeng.dao.ClassinfoEntity" table="classinfo" schema="exam">
        <id name="classId">
            <column name="classId" sql-type="int(11)"/>
        </id>
        <property name="className">
            <column name="className" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="gradeId">
            <column name="GradeId" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>