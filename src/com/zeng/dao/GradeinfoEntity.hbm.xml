<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zeng.dao.GradeinfoEntity" table="gradeinfo" schema="exam">
        <id name="gradeId">
            <column name="gradeId" sql-type="int(11)"/>
        </id>
        <property name="gradeName">
            <column name="GradeName" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>